<GVPSResponse>
	<Mode>Response </Mode>
	<Order>
		<OrderID>Response OrderID</OrderID>
		<GroupID>Response GroupID</GroupID>
		<OrderInqResult>		
			<ChargeType>ResponseOrderOrderInqResult ChargeType</ChargeType>
			<PreAuthAmount>ResponseOrderOrderInqResult PreAuthAmount</PreAuthAmount>
			<PreAuthDate>ResponseOrderOrderInqResult PreAuthDate</PreAuthDate>
			<AuthAmount>ResponseOrderOrderInqResult AuthAmount</AuthAmount>
			<AuthDate>ResponseOrderOrderInqResult AuthDate</AuthDate>
		</OrderInqResult>
		<OrderHistInqResult>
			<OrderTxnList>
				<Count></Count>
				<OrderTxn>
					<Type></Type>
					<Status></Status>
					<PreAuthAmount></PreAuthAmount>
					<AuthAmount></AuthAmount>
					<PreAuthDate></PreAuthDate>
					<AuthDate></AuthDate>
					<VoidDate></VoidDate>
					<RetrefNum></RetrefNum>
					<AuthCode></AuthCode>
					<ReturnCode></ReturnCode>
					<BatchNum></BatchNum>
				</OrderTxn>
			</OrderTxnList>
		</OrderHistInqResult>
	</Order>
	<Transaction>
		<Response>
			<Source>HOST</Source>
			<Code>96</Code> <!--00 dan baska bir kod hata demek-->
			<ReasonCode>9600</ReasonCode>
			<Message>Declined</Message>
			<ErrorMsg>Expire card</ErrorMsg>
			<SysErrMsg>Son kullanma tarihi hatali</SysErrMsg>
		</Response>
		<RetrefNum>ResponseTransaction RetrefNum</RetrefNum>
		<AuthCode>ResponseTransaction AuthCode</AuthCode>
		<BatchNum>ResponseTransaction BatchNum</BatchNum>
		<SequenceNum>ResponseTransaction SequenceNum</SequenceNum>
		<ProvDate>ResponseTransaction ProvDate</ProvDate>
		<CardNumberMasked>ResponseTransaction CardNumberMasked</CardNumberMasked>
		<HostMsgList>
			<HostMsg>ResponseTransactionHostMsg HostMsg1</HostMsg>
		</HostMsgList>
		<ExtendedCreditResult>
			<TotalAmount></TotalAmount>
			<InterestAmount></InterestAmount>
			<BITTAmount></BITTAmount>
			<RUSFAmount></RUSFAmount>
			<InterestRate></InterestRate>
			<BITTRate></BITTRate>
			<RUSFRate></RUSFRate>
			<InstallmentCnt></InstallmentCnt>
			<PaymentList>
				<Payment> 
					<PaymentNumber></PaymentNumber>
					<Amount></Amount>					
					<DueDate></DueDate>
				</Payment>
			</PaymentList>
		</ExtendedCreditResult>
		<RewardInqResult>
			<RewardList>
				<Reward>
					<Type>MR, Bonus, Mil, FirmBasedBns</Type>
					<TotalAmount></TotalAmount>
					<LastTxnGainAmount></LastTxnGainAmount>
				</Reward>
			</RewardList>
			<ChequeList>
				<Cheque>
					<Type>Player / Sozunuze Urun</Type>
					<Amount></Amount>					
					<Count></Count> <!-- Player -->					
					<ExpireDate></ExpireDate>
					<UsageRate></UsageRate>
					<MinTxnAmount></MinTxnAmount>
					<ID></ID> <!-- Sozunuze Urun -->
					<Bitmap></Bitmap>
				</Cheque>
			</ChequeList>
		</RewardInqResult>
		<UtilityPaymentInqResult>
			<SubscriberCode></SubscriberCode>
			<SubscriberName></SubscriberName>
			<InvoiceCount></InvoiceCount>
			<InvoiceList>
				<Invoice>
					<ID></ID>
					<DueDate></DueDate>
					<Amount></Amount>
					<AmountChangeInd></AmountChangeInd> 
					<CommissionAmount></CommissionAmount>
					<RewardAmount></RewardAmount>
					<RetrefNum></RetrefNum>				
				</Invoice>
			</InvoiceList>
		</UtilityPaymentInqResult>
		<GSMUnitInqResult>
			<SubscriberCode></SubscriberCode>
			<SubscriberName></SubscriberName>
			<ItemCount></ItemCount>
			<ItemList>
				<Item>
					<UnitID></UnitID>
					<Quantity></Quantity>
					<Amount></Amount>
					<CommissionAmount></CommissionAmount>
				</Item>
			</ItemList>
		</GSMUnitInqResult>
		<CepBankInqResult>
			<GSMNumber></GSMNumber>
			<TxnStatusCode></TxnStatusCode>
			<SentClient></SentClient>
			<PaymentType>Credit / Debit / Account</PaymentType>
			<TxnAmount></TxnAmount>
			<BonusAmount></BonusAmount>
			<InstallmentCnt></InstallmentCnt>
			<ResponseCode></ResponseCode>
			<Message></Message>
		</CepBankInqResult>		
	</Transaction>
	<SettlementInqResult>
		<Date></Date>
		<TransactionSummList>
			<TransactionSumm>
				<CurrencyCode></CurrencyCode>			
				<Type></Type>
				<Count></Count>
				<Amount></Amount>
			</TransactionSumm>
		</TransactionSummList>
	</SettlementInqResult>
</GVPSResponse>